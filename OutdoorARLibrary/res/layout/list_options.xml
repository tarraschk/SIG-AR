<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/relativelayoutmain" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:gravity="center">
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:orientation="vertical"
		android:id="@+id/LinearLayoutColumn">
		<LinearLayout android:id="@+id/LinearLayoutLine1"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:gravity="center">
			<TableLayout android:layout_height="wrap_content" android:gravity="center"
				android:layout_margin="10dip" android:background="@drawable/transborder"
				android:layout_width="80dip" android:visibility="gone" android:id="@+id/TableLayoutManage">
				<RelativeLayout android:id="@+id/RelativeLayout01"
					android:layout_height="fill_parent" android:layout_width="fill_parent"
					android:gravity="center">
					<ImageButton android:background="@drawable/aroundme_thumb"
						android:layout_height="60dip" android:layout_width="60dip"
						android:layout_gravity="center" android:id="@+id/ImageButtonManage"></ImageButton>
				</RelativeLayout>
				<TextView android:id="@+id/TextView01"
					android:layout_height="wrap_content" android:gravity="center"
					android:layout_width="wrap_content" android:text="Manage"
					android:textSize="13dip" android:textStyle="bold"
					android:textColor="@color/white"></TextView>
			</TableLayout>
			<TableLayout android:layout_height="wrap_content" android:gravity="center"
				android:layout_margin="10dip" android:background="@drawable/transborder"
				android:layout_width="80dip" android:id="@+id/TableLayoutShare" android:visibility="gone">
				<RelativeLayout android:id="@+id/RelativeLayout01"
					android:layout_height="fill_parent" android:layout_width="fill_parent"
					android:gravity="center">
					<ImageButton android:background="@drawable/aroundme_thumb"
						android:layout_height="60dip" android:layout_width="60dip"
						android:layout_gravity="center" android:id="@+id/ImageButtonShare"></ImageButton>
				</RelativeLayout>
				<TextView android:id="@+id/TextView01"
					android:layout_height="wrap_content" android:gravity="center"
					android:layout_width="wrap_content" android:textSize="13dip" android:textStyle="bold"
					android:textColor="@color/white" android:text="Share"></TextView>
			</TableLayout>

		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayoutLine2"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:gravity="center">
			<TableLayout android:layout_height="wrap_content" android:gravity="center"
				android:layout_margin="10dip" android:background="@drawable/transborder"
				android:layout_width="80dip" android:id="@+id/TableLayoutARView">
				<RelativeLayout android:id="@+id/RelativeLayout01"
					android:layout_height="fill_parent" android:layout_width="fill_parent"
					android:gravity="center">
					<ImageButton android:background="@drawable/aroundme_thumb"
						android:layout_height="60dip" android:layout_width="60dip"
						android:layout_gravity="center" android:id="@+id/ImageButtonARView"></ImageButton>
				</RelativeLayout>
				<TextView android:id="@+id/TextView01"
					android:layout_height="wrap_content" android:gravity="center"
					android:layout_width="wrap_content" android:textSize="13dip" android:textStyle="bold"
					android:textColor="@color/white" android:text="AR View"></TextView>
			</TableLayout>
			<TableLayout android:layout_height="wrap_content" android:gravity="center"
				android:layout_margin="10dip" android:background="@drawable/transborder"
				android:layout_width="80dip" android:id="@+id/TableLayoutMapView">
				<RelativeLayout android:id="@+id/RelativeLayout01"
					android:layout_height="fill_parent" android:layout_width="fill_parent"
					android:gravity="center">
					<ImageButton android:background="@drawable/aroundme_thumb"
						android:layout_height="60dip" android:layout_width="60dip"
						android:layout_gravity="center" android:id="@+id/ImageButtonMapView"></ImageButton>
				</RelativeLayout>
				<TextView android:id="@+id/TextView01"
					android:layout_height="wrap_content" android:gravity="center"
					android:layout_width="wrap_content" android:textSize="13dip" android:textStyle="bold"
					android:textColor="@color/white" android:text="Map View"></TextView>
			</TableLayout>
		</LinearLayout>
	</LinearLayout>
</RelativeLayout>