<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:gravity="center" android:padding="20dip">
	<TableLayout android:layout_below="@id/RelativeLayout01" android:background="@drawable/markerbordermap" android:layout_height="wrap_content" android:layout_width="wrap_content" android:clickable="true" android:id="@+id/TableLayoutViewFurtherInformation">
		<RelativeLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/Title">
			<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:gravity="center" android:layout_width="wrap_content">
				
				
			




<RelativeLayout android:id="@+id/RelativeLayout02" android:layout_height="wrap_content" android:gravity="center" android:layout_width="wrap_content"><ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Image" android:layout_toLeftOf="@+id/TitleText" android:clickable="true"></ImageView></RelativeLayout><RelativeLayout android:id="@+id/RelativeLayout04" android:layout_height="wrap_content" android:layout_width="match_parent" android:animationCache="true"><RelativeLayout android:id="@+id/RelativeLayout01" android:gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:text="@+id/TextView02" android:id="@+id/TitleText" android:textSize="14dip" android:textColor="@color/white" android:textStyle="bold" android:layout_toRightOf="@+id/Image" android:gravity="center" android:layout_toLeftOf="@+id/Quit" android:layout_width="match_parent" android:layout_height="60dip"></TextView></RelativeLayout><RelativeLayout android:id="@+id/RelativeLayout03" android:gravity="top|right" android:layout_width="match_parent" android:layout_height="wrap_content"><ImageButton android:id="@+id/Quit" android:layout_height="60dip" android:layout_width="60dip"></ImageButton>

</RelativeLayout></RelativeLayout>
</TableRow>
		</RelativeLayout>
		<RelativeLayout android:layout_width="wrap_content" android:id="@+id/Cursor1" android:layout_height="2dip" android:background="@drawable/markerbordermap" android:padding="10dip" android:layout_marginBottom="10dip" android:layout_marginLeft="20dip" android:layout_marginRight="20dip" android:layout_marginTop="10dip">
</RelativeLayout><RelativeLayout android:layout_below="@id/Title"
			android:layout_width="wrap_content" android:id="@+id/Body" android:layout_height="fill_parent">
			<ScrollView android:id="@+id/ScrollView01"
				android:layout_height="wrap_content" android:layout_width="match_parent">
				<TextView android:text="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/bodyText" android:textColor="@color/white" android:textStyle="normal|italic" android:textSize="12dip"></TextView>
			</ScrollView>
		</RelativeLayout>
		<RelativeLayout android:layout_width="wrap_content" android:id="@+id/Cursor" android:background="@drawable/markerbordermap" android:layout_height="2dip" android:layout_marginBottom="10dip" android:layout_marginTop="10dip" android:layout_marginRight="20dip" android:layout_marginLeft="20dip" android:visibility="gone"></RelativeLayout><RelativeLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/End" android:visibility="gone">
			
		
<TableRow android:id="@+id/TableRow02" android:layout_height="wrap_content" android:gravity="center" android:layout_width="fill_parent"><Button android:layout_below="@id/Body" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/endButton" android:text="Close"></Button></TableRow>
</RelativeLayout>
	
</TableLayout>




</RelativeLayout>
